{
  "name": "blogial-client",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "host": "ng serve --host 192.168.0.3",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run blogial-client:server",
    "webpack:server": "webpack --config webpack.server.config.js --colors",
    "start:heroku": "npm run serve:ssr",
    "stats": "webpack-bundle-analyzer ./dist/browser/stats.json",
    "postinstall": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.4",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/platform-server": "^6.0.9",
    "@angular/pwa": "^0.8.1",
    "@angular/router": "^6.0.4",
    "@angular/service-worker": "^6.0.4",
    "@fortawesome/angular-fontawesome": "^0.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^2.2.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "bootstrap": "^4.1.2",
    "core-js": "^2.5.4",
    "helmet": "^3.13.0",
    "ngrx-store-freeze": "^0.2.4",
    "ngrx-store-localstorage": "^5.0.1",
    "ngx-chips": "^1.9.7",
    "ngx-md": "^6.0.0",
    "rxjs": "^6.0.0",
    "ts-loader": "^4.4.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
